var contact_message_elements = $('.contact.form .form-message')

<% if @success %>
  contact_message_elements.removeClass('negative').addClass('positive')
<% else %>
  contact_message_elements.removeClass('positive').addClass('negative')
<% end %>

contact_message_elements.text('<%= @message %>').show()
Forms.set_form_loading($('.contact.form'), false)
console.log('Contact returned', '<%= @success %>', '<%= @message %>', contact_message_elements)
